var move_speed = 7; // Ajusta la velocidad de movimiento vertical
var half_height = sprite_height / 2;

if(y - half_height &lt; view_yview){
   y = view_yview + half_height;
}

if(y + half_height &gt; view_yview + view_hview){
   y = view_yview + view_hview - half_height;
}

if (keyboard_check(vk_up)) {
